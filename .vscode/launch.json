    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Load Gen",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${fileDirname}",
            "env": {
                "base_url": "http://www.micromicroservice.net"
            }
        },
        {
            "name": "Python: FastAPI - Name Micro",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "cwd": "${workspaceFolder}",
            "args": [
                "app.name_micro:app"
            ],
            "jinja": true
        },
        {
            "name": "Python: FastAPI - Name Main",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "cwd": "${workspaceFolder}",
            "args": [
                "app.name_main:app"
            ],
            "jinja": true
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "cwd": "${fileDirname}",
            "console": "integratedTerminal"
        }
    ]
}